{
    "name": "filament/avatar-chat",
    "description": "A Filament plugin for avatar chat features",
    "type": "filament-plugin",
    "version": "0.1.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Filament\\AvatarChat\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\AvatarChat\\AvatarChatServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "laravel/framework": "^11.0"
    },
    "minimum-stability": "stable", 
    "prefer-stable": true 
}